{
  "name": "wyrihaximus/react-sync-configmap-mount-to-env",
  "description": "\ud83d\udd75\ufe0f\u200d\u2640\ufe0f Monitors and syncs configmap mount into environment variables",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "react/async": "^4@dev",
    "react/event-loop": "^1.3",
    "react/filesystem": "^0.2@dev"
  },
  "require-dev": {
    "wyrihaximus/async-test-utilities": "^5.0.3"
  },
  "autoload": {
    "psr-4": {
      "WyriHaximus\\React\\Env\\ConfigMapMountSyncer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WyriHaximus\\Tests\\React\\Env\\ConfigMapMountSyncer\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "icanhazstring/composer-unused": true,
      "infection/extension-installer": true,
      "composer-unused/composer-unused-plugin": true,
      "wyrihaximus/composer-update-bin-autoload-path": true
    },
    "platform": {
      "php": "8.1.13"
    },
    "sort-packages": true
  },
  "scripts": {
    "post-install-cmd": [
      "composer normalize"
    ],
    "post-update-cmd": [
      "composer normalize"
    ]
  }
}
